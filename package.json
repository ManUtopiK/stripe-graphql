{
  "name": "stripe-graphql",
  "version": "0.2.0",
  "description": "Stripe API implementation in GraphQL.",
  "main": "src/index.ts",
  "repository": "https://github.com/jferrettiboke/stripe-graphql.git",
  "author": "Jes√∫s Ferretti <jferrettiboke@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config src/server.ts",
    "gen:bindings": "graphql-binding --language typescript --input ./src/schema.ts --outputBinding ./src/generated/binding.ts",
    "gen:types": "ts-node-dev -r dotenv/config src/generate-types.ts"
  },
  "dependencies": {
    "glob": "^7.1.4",
    "graphql": "^14.3.1",
    "graphql-binding": "^2.3.5",
    "nexus": "^0.12.0-beta.4",
    "stripe": "^7.1.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/prettier": "^1.16.4",
    "@types/stripe": "^6.26.5",
    "dotenv": "^8.0.0",
    "graphql-yoga": "^1.16.7",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.5.1"
  }
}
